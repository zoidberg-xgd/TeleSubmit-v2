# Telegram机器人令牌
TOKEN=your_bot_token_here

# 频道ID（格式为@channel_username或-100开头的ID）
CHANNEL_ID=@your_channel_name

# 机器人所有者ID（数字）
OWNER_ID=your_user_id_here

# 会话超时时间（秒）
SESSION_TIMEOUT=300

# 数据库路径
DB_PATH=submissions.db

# 机器人运行模式 (MEDIA/DOCUMENT/MIXED)
BOT_MODE=MIXED

# 是否显示投稿人信息 (True/False)
SHOW_SUBMITTER=True

# 是否向所有者发送通知 (True/False)
NOTIFY_OWNER=True 